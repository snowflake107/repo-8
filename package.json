    "actions",
    "node",
    "setup"
  ],
  "author": "@dgschwindturo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/types": "^5.5.0",
    "got": "^11.8.1",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/parser": "^4.14.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.21.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.2.3"
  }
